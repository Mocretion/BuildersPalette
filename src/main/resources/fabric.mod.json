{
	"schemaVersion": 1,
	"id": "blockpalettes",
	"version": "${version}",
	"name": "BlockPalettes",
	"description": "Select a previously defined block palette to automatically build with.",
	"authors": [
		"Mocretion"
	],
	"contact": {
		"homepage": "https://github.com/Mocretion",
		"sources": "https://github.com/Mocretion/BlockPalettes"
	},
	"license": "GNU LGPLv3",
	"icon": "assets/blockpalettes/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.mocretion.blockpalettes.BlockPalettes"
		],
		"client": [
			"com.mocretion.blockpalettes.BlockPalettesClient"
		]
	},
	"mixins": [
		"blockpalettes.mixins.json",
		{
			"config": "blockpalettes.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}